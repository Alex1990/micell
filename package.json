{
  "name": "micell",
  "version": "0.1.0",
  "description": "A set of functions for web development",
  "main": "lib/index.js",
  "module": "es/index.js",
  "engines": {
    "node": ">=6.0.0"
  },
  "files": [
    "dist",
    "lib",
    "es",
    "src"
  ],
  "scripts": {
    "clean": "rimraf dist lib es",
    "lint": "standard '**/*.js' --verbose | snazzy",
    "lint:fix": "standard '**/*.js' --fix --verbose | snazzy",
    "test": "cross-env BABEL_ENV=test karma start",
    "test:ci": "npm run test -- --single-run",
    "build": "npm run clean && npm run build:es && npm run build:commonjs && npm run build:umd && npm run build:umd:min",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "build:es": "cross-env babel src --out-dir es",
    "build:umd": "cross-env NODE_ENV=development webpack",
    "build:umd:min": "cross-env NODE_ENV=production webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Alex1990/micell.git"
  },
  "standard": {
    "env": [
      "node",
      "browser",
      "mocha"
    ],
    "globals": [
      "chai",
      "sinon",
      "expect"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "standard --fix",
      "git add"
    ]
  },
  "keywords": [
    "micellar",
    "utility",
    "util"
  ],
  "author": "Alex Chao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Alex1990/micell/issues"
  },
  "homepage": "https://github.com/Alex1990/micell#readme",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-transform-modules-commonjs": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "babel-loader": "^8.0.5",
    "chai": "^4.2.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "husky": "^2.1.0",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-file-fixtures-preprocessor": "^2.0.1",
    "karma-mocha": "^1.3.0",
    "karma-webpack": "^4.0.2",
    "lint-staged": "^8.1.5",
    "mkdirp": "^0.5.1",
    "mocha": "^6.1.4",
    "request": "^2.88.0",
    "rimraf": "^2.6.1",
    "sinon": "^7.3.2",
    "snazzy": "^8.0.0",
    "standard": "^12.0.1",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1"
  },
  "precommit": [
    "lint"
  ],
  "dependencies": {
    "@babel/runtime": "^7.4.4"
  }
}
